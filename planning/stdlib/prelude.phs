# Stack
{ $_ = } $pop =				# Pop value
{ $v = v v } $dup =			# Duplicate value
{ { $a $b } = b a } $swap =	# swap values

# Comparisons
{ 1 } { == ! } $!= global fun	# Not equals
{ 1 } { > ! } $<= global fun	# LE
{ 1 } { < ! } $>= global fun	# GE

# Booleans
1 $true  =
0 $false =

# Logic
# TODO use WASM
{ 1 } { 0 == } $! global fun	        # Not
{ type I64 == } { 0L } $! global fun
{ 1 } { * ! ! } $&& global fun	        # And
{ 1 } { + ! ! } $|| global fun	        # Or
