
{ # Intentionally non-tail-recursive factorial
    $n =
    { 1 } { n 1 - fac n * } $branch fun
    { n 2 < } { 1 } $branch fun
    branch
} rec $fac =

{ I32 } { fac } $fac export

{
    $n =
    { 1 } { n 1 - fib n 2 - fib + } $branch fun
    { n 2 < } { 1 } $branch fun
    branch
} rec $fib =
{ I32 } { fib } $fib export