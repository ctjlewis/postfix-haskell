# Stack
{ $_ = } $pop =				# Pop value
{ $v = v v } $dup =			# Duplicate value
{ $b = $a = b a } $swap =	# swap values

# Comparisons
{ 1 } { == ! } $!= global fun	# Not equals
{ 1 } { > ! } $<= global fun	# LE
{ 1 } { < ! } $>= global fun	# GE

# Booleans
1 $true =
0 $false =

# Logic
{ 1 } { 0 == } $! global fun	# Not
{ 1 } { * 0 != } $&& global fun	# And
{ 1 } { + 0 != } $|| global fun	# Or
